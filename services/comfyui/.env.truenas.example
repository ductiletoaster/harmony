# TrueNAS Dockge Environment Variables for ComfyUI
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=true

# Data Paths (TrueNAS)
COMFY_DATA_PATH=/mnt/Media/libraries/dockge/containers/comfyui

# User/Group IDs
PUID=1000
PGID=1000

# ComfyUI Configuration
COMFY_PORT=8188

# GPU Configuration
GPU_DRIVER=nvidia
GPU_COUNT=all
GPU_CAPABILITIES=gpu 