# Local Development Environment Variables for ComfyUI
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=false

# Data Paths (Local)
COMFY_DATA_PATH=./data

# User/Group IDs
PUID=1000
PGID=1000

# ComfyUI Configuration
COMFY_PORT=8188

# GPU Configuration
GPU_DRIVER=nvidia
GPU_COUNT=all
GPU_CAPABILITIES=gpu 