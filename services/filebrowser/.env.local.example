# Local Development Environment Variables for Filebrowser
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=false

# Data Paths (Local)
FILEBROWSER_STORAGE_PATH=./storage

# Volume Names (Local)
FILEBROWSER_CONFIG_VOLUME=filebrowser_config
FILEBROWSER_DATABASE_VOLUME=filebrowser_database
FILEBROWSER_CONFIG_NAME=filebrowser_config
FILEBROWSER_DATABASE_NAME=filebrowser_database

# User/Group IDs
PUID=1000
PGID=1000 