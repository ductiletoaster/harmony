# TrueNAS Dockge Environment Variables for Filebrowser
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=true

# Data Paths (TrueNAS)
FILEBROWSER_STORAGE_PATH=/mnt/Media/libraries/dockge/containers/filebrowser

# Volume Names (TrueNAS)
FILEBROWSER_CONFIG_VOLUME=/mnt/Media/libraries/dockge/filebrowser
FILEBROWSER_DATABASE_VOLUME=/mnt/Media/libraries/dockge/filebrowser
FILEBROWSER_CONFIG_NAME=filebrowser_config
FILEBROWSER_DATABASE_NAME=filebrowser_database

# User/Group IDs
PUID=1000
PGID=1000 