# Local Development Environment Variables for Open WebUI
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=false

# Data Paths (Local)
OPEN_WEBUI_DATA_PATH=./open-webui
OLLAMA_DATA_PATH=./ollama
FASTER_WHISPER_DATA_PATH=./faster-whisper

# User/Group IDs
PUID=1000
PGID=1000

# Ollama Configuration
OLLAMA_KEEP_ALIVE=24h

# Faster Whisper Configuration
WHISPER_MODEL=tiny-int8
WHISPER_BEAM=1
WHISPER_LANG=en

# GPU Configuration (uncomment and configure as needed)
# GPU_DRIVER=nvidia
# GPU_COUNT=all
# GPU_CAPABILITIES=gpu 