# Immich Service - TrueNAS Dockge Environment Configuration
# Copy this file to .env and customize it for your TrueNAS deployment

# Domain Configuration
HOSTNAME=s01.pixeloven.com

# File Permissions
PUID=1000
PGID=1000

# Timezone
TZ=America/New_York

# Immich Version
IMMICH_VERSION=release

# Database Configuration
DB_USERNAME=postgres
DB_PASSWORD=your-secure-db-password-here
DB_DATABASE_NAME=immich

# Redis Configuration
REDIS_PASSWORD=your-secure-redis-password-here

# TrueNAS Volume Paths
# Image/Video library path (host mount)
# Note: Database, Redis, and ML models use named Docker volumes
IMMICH_UPLOAD_PATH=/mnt/Media/libraries/dockge/containers/immich/library

# Optional Features
DISABLE_REVERSE_GEOCODING=false
REVERSE_GEOCODING_PRECISION=3
LOG_LEVEL=log
ENABLE_DIAGNOTICS=true
