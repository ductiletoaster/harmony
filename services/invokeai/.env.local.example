# Local Development Environment Variables for InvokeAI
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=false

# Data Paths (Local)
INVOKEAI_MODELS_PATH=./models
INVOKEAI_OUTPUTS_PATH=./outputs
INVOKEAI_CONFIGS_PATH=./configs
INVOKEAI_DATABASES_PATH=./databases

# GPU Configuration
GPU_DRIVER=nvidia
GPU_COUNT=all
GPU_CAPABILITIES=gpu 