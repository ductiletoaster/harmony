# TrueNAS Dockge Environment Variables for InvokeAI
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=true

# Data Paths (TrueNAS)
INVOKEAI_MODELS_PATH=/mnt/Media/libraries/dockge/containers/invokeai/models
INVOKEAI_OUTPUTS_PATH=/mnt/Media/libraries/dockge/containers/invokeai/outputs
INVOKEAI_CONFIGS_PATH=/mnt/Media/libraries/dockge/invokeai
INVOKEAI_DATABASES_PATH=/mnt/Media/libraries/dockge/invokeai

# GPU Configuration
GPU_DRIVER=nvidia
GPU_COUNT=all
GPU_CAPABILITIES=gpu 