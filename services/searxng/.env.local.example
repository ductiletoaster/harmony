# Local Development Environment Variables for SearXNG
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=false

# Data Paths (Local)
SEARXNG_CONFIG_PATH=./config
SEARXNG_DATA_VOLUME=searxng_data
VALKEY_DATA_VOLUME=valkey_data

# User/Group IDs
PUID=1000
PGID=1000

# SearXNG Configuration
SEARXNG_HOSTNAME=searxng.docker.localhost
SEARXNG_PORT=8080
SEARXNG_REMOTE_SETUP=0 