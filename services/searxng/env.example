# SearXNG Service - Generic Environment Configuration
# Copy this file to .env and customize it for your deployment

# File Permissions
PUID=1000
PGID=1000

# Service Configuration
SEARXNG_HOSTNAME=searxng.docker.localhost
SEARXNG_PORT=8080
SEARXNG_REMOTE_SETUP=0

# Data Paths (Customize for your deployment)
SEARXNG_CONFIG_PATH=./config

# Volume Names
SEARXNG_DATA_VOLUME=searxng_data
VALKEY_DATA_VOLUME=valkey_data 