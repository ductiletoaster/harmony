# Local Development Environment Variables for Syncthing
# Copy this to .env and modify as needed

# Network Configuration
PROXY_NETWORK=proxy
PROXY_NETWORK_EXTERNAL=false
SYNCTHING_NETWORK_NAME=syncthing_network

# Data Paths (Local)
SYNCTHING_CONFIG_PATH=./config
SYNCTHING_STORAGE_PATH=./storage

# User/Group IDs
PUID=1000
PGID=1000

# Timezone
TZ=Etc/UTC 